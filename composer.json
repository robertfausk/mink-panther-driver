{
  "name": "robertfausk/mink-panther-driver",
  "description": "Symfony Panther driver for Mink framework",
  "keywords": [
    "Symfony",
    "Mink",
    "testing",
    "panther",
    "chrome",
    "chromium",
    "firefox",
    "headless",
    "browser"
  ],
  "homepage": "http://mink.behat.org/",
  "type": "mink-driver",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Freigang",
      "email": "robertfreigang@gmx.de"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/robertfausk/panther"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-dom": "*",
    "behat/mink": "~1.8",
    "symfony/panther": "~0.7|~1.0|~2.0"
  },
  "require-dev": {
    "dbrekelmans/bdi": "^1.0",
    "mink/driver-testsuite": "dev-master",
    "phpunit/phpunit": "~8.5|~9.3",
    "symfony/dom-crawler": "~4.0|~5.0|~6.0",
    "symfony/http-kernel": "~4.0|~5.0|~6.0"
  },
  "scripts": {
      "qa": ""
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pyrech/composer-changelogs": true
    }
  },
  "suggest": {
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "Behat\\Mink\\Driver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Behat\\Mink\\Tests\\Driver\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.1.x-dev"
    }
  }
}
